{
    "componentName": "LlavaDetection",
    "componentVersion": "9.0",
    "middlewareVersion": "9.0",
    "sourceLanguage": "python",
    "batchLibrary": "LlavaDetection",
    "environmentVariables": [],
    "algorithm": {
        "name": "LLaVA",
        "description": "LLaVA prompt response generation.",
        "actionType": "DETECTION",
        "trackType": "CLASS",
        "outputChangedCounter": 1,
        "requiresCollection": {
            "states": []
        },
        "providesCollection": {
            "states": [
                "DETECTION",
                "DETECTION_CLASS",
                "DETECTION_CLASS_LLAVA"
            ],
            "properties": [
                {
                    "name": "PROMPT_CONFIGURATION_PATH",
                    "description": "Path to a custom JSON file which contains the classes and associated prompts that will be sent to LLaVA.",
                    "type": "STRING",
                    "defaultValue": ""
                },
                {
                    "name": "OLLAMA_CLIENT_HOST_URL",
                    "description": "Something",
                    "type": "STRING",
                    "defaultValue": "llava-detection-server:11434"
                }
            ] 
        }
    },
    "actions": [
        {
            "name": "LLAVA DETECTION (WITH FF REGION) ACTION",
            "description": "Runs LLaVA with prompts for the class of the detection passed in.",
            "algorithm": "LLaVA",
            "properties": [
                {
                    "name": "FEED_FORWARD_TYPE",
                    "value": "REGION"
                }
            ]
        }
    ],
    "tasks": [
        {
            "name": "LLAVA DETECTION (WITH FF REGION) TASK",
            "description": "Runs LLaVA with prompts for the class of the detection passed in.",
            "actions": [
                "LLAVA DETECTION (WITH FF REGION) ACTION"
            ]
        }
    ],
    "pipelines": [
        {
            "name": "LLAVA DETECTION (WITH FF REGION) PIPELINE",
            "description": "Runs LLaVA with prompts for the class of the detection passed in.",
            "tasks": [
                "LLAVA DETECTION (WITH FF REGION) TASK"
            ]
        }
    ]
}
